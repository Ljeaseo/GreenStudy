<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mapper.studylistMapper">

<select id="studylist" resultType="com.test.model.studylistVO">
	select title
	from studylist
	order by cnt desc 
	limit 1
</select>

</mapper>